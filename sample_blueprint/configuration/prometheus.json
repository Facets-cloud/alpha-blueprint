{
  "kind": "configuration",
  "for": "prometheus",
  "version": "latest",
  "lifecycle": "ENVIRONMENT_BOOTSTRAP",
  "disabled": false,
  "provided": true,
  "spec": {
    "diskSize": "100G",
    "size": {
      "cpu": 1,
      "memory": 1
    }
  },
  "out": {
    "comment": "Will be generated, or can be provided",
    "interface": {
      "prometheus": {
        "port": 1000,
        "host": "prometheus.namespace",
        "username": "user",
        "password": "password"
      },
      "alertManager": {
        "port": 1000,
        "host": "alert-manager.namespace",
        "username": "user",
        "password": "password"
      }
    }
  }
}