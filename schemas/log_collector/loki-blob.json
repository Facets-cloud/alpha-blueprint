{
    "$schema": "./log-collector.schema.json",
    "flavor": "loki_blob",
    "metadata": {
      "name": "loki-blob-log-collector"
    },
    "kind": "log_collector",
    "provided": false,
    "disabled": true,
    "version": "latest",
    "spec": {
        "retentation_days": 10,
        "storage_size": "10"
    },
    "advanced": {
      "loki_blob": {
        "container_name": "${azure_storage_container.test-azure-container.out.attributes.container_name}",
        "storage_account_name": "${azure_storage_container.test-azure-container.out.attributes.storage_account_name}",
        "primary_access_key": "${azure_storage_container.test-azure-container.out.attributes.primary_access_key}"
      }
    }
  }
  