{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definition": "Cloudsql mysql Schema",
  "type": "object",
  "properties": {
    "cloudsql": {
      "type": "object",
      "description": "Advanced section of the cloudsql",
      "properties": {
        "mysql": {
          "type": "object",
          "description": "Advanced values based on https://github.com/terraform-google-modules/terraform-google-sql-db/tree/master/modules/mysql",
          "properties": {
            "reader_zone": {
              "type": "object",
              "description": "An object containing reader properties, with each reader represented as a property named reader-0, reader-1, etc.",
              "patternProperties": {
                "^reader-[0-9]+$": {
                  "type": "string"
                }
              },
              "required": []
            },
            "writer_zone": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "activation_policy": {
              "type": "string"
            },
            "availability_type": {},
            "read_replica_deletion_protection": {
              "type": "string"
            },
            "read_replica_name_suffix": {
              "type": "string"
            },
            "db_charset": {
              "type": "string"
            },
            "db_collation": {
              "type": "string"
            },
            "db_name": {
              "type": "string"
            },
            "delete_timeout": {
              "type": "string"
            },
            "deletion_protection": {
              "type": "string"
            },
            "disk_type": {
              "type": "string"
            },
            "enable_default_db": {
              "type": "string"
            },
            "enable_default_user": {
              "type": "string"
            },
            "encryption_key_name": {
              "type": "string"
            },
            "maintenance_window_day": {
              "type": "string"
            },
            "maintenance_window_hour": {
              "type": "string"
            },
            "maintenance_window_update_track": {
              "type": "string"
            },
            "random_instance_name": {
              "type": "string"
            },
            "secondary_zone": {
              "type": "string"
            },
            "module_depends_on": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "database_flags": {
              "type": "object",
              "description": "An object containing the database flags, with each flag represented as a property named flag1, flag2, etc.",
              "patternProperties": {
                "^flag[0-9]+$": {
                  "type": "object",
                  "description": "An individual flag, with a name and value property.",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "value": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "name",
                    "value"
                  ]
                }
              }
            },
            "backup_configuration": {
              "type": "object",
              "properties": {
                "binary_log_enabled": {
                  "type": "boolean"
                },
                "enabled": {
                  "type": "boolean"
                },
                "start_time": {
                  "type": "string"
                },
                "location": {
                  "type": "string"
                },
                "transaction_log_retention_days": {
                  "type": "string"
                },
                "retained_backups": {
                  "type": "number"
                },
                "retention_unit": {
                  "type": "string"
                }
              },
              "required": []
            },
            "ip_configuration": {
              "type": "object",
              "properties": {
                "authorized_networks": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "ipv4_enabled": {
                  "type": "boolean"
                },
                "private_network": {
                  "type": "string"
                },
                "require_ssl": {
                  "type": "boolean"
                },
                "allocated_ip_range": {
                  "type": "string"
                }
              },
              "required": []
            },
            "insights_config": {
              "type": "object",
              "properties": {
                "query_string_length": {
                  "type": "number"
                },
                "record_application_tags": {
                  "type": "boolean"
                },
                "record_client_address": {
                  "type": "boolean"
                }
              },
              "required": []
            },
            "additional_users": {
              "type": "object",
              "properties": {
                "user1": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "password": {
                      "type": "string"
                    },
                    "host": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "name",
                    "password",
                    "host"
                  ]
                }
              },
              "required": [
                "user1"
              ]
            },
            "additional_databases": {
              "type": "object",
              "description": "An object containing database properties, with each database represented as a property named db1,db2, etc.",
              "patternProperties": {
                "^db[0-9]+$": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "name"
                  ]
                },
                "db2": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "name"
                  ]
                }
              },
              "required": [
              ]
            },
            "user_labels": {
              "type": "object",
              "description": "An object containing key-value pairs for user labels.",
              "additionalProperties": {
                "type": "string",
                "description": "The value of an individual label."
              }
            }
          },
          "required": [
          ]
        }
      },
      "required": [
        "mysql"
      ]
    }
  },
  "required": [
    "cloudsql"
  ]
}