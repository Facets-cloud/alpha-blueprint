{
    "$schema": "./nlb.schema.json",
    "kind": "tcp_lb",
    "flavor": "nlb",
    "metadata": {
        "name": "test"
    },
    "version": "0.1",
    "spec": {
        "mode": "loadbalancing",
        "instances": 1,
        "ports": {
            "ports1": {
                "port": 80,
                "protocol": "tcp"
            }
        },
        "private": false,
        "selector": {
            "app.kubernetes.io": "app1"
        }

    }

}