{
  "$schema":"./loadbalancer.schema.json",
  "kind":"loadbalancer",
  "flavor":"default",
  "version":"latest",
  "lifecycle":"ENVIRONMENT_BOOTSTRAP",
  "disabled":false,
  "provided":false,
  "metadata":{
     
  },
  "spec":{
     "private":true,
     "force_ssl_redirection":true,
     "authenticated":true,
     "size":{
        "cpu":1,
        "memory":1
     },
     "domains":{
        "mpl-main":{
           "certificate_reference":"Secret Name",
           "domain":"mpl.com",
           "alias":"somealias"
        }
     },
     "rules":{
        "api-routing":{
           "path":"/",
           "domain_prefix":"app1",
           "service_name":"${application.app1.out.interfaces.grpc.host}",
           "port":80
        }
     }
  },
  "out":{
     "interfaces":{
        "app1":{
           "host":"hostname with domain prefix of domains",
           "port":443
        }
     }
  }
}