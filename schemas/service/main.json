{
    "$schema": "./service.schema.json",
    "disabled": false,
    "version": "0.1",
    "metadata": {},
    "flavor": "default",
    "kind": "service",
    "spec": {
        "type": "application",
        "enable_host_anti_affinity": true,
        "release": {
            "image": "",
            "strategy": {
                "type": "Recreate"
            }
        },
        "runtime": {
            "volumes": {
                "configs": {
                    "ssl-certificate-1": {
                        "mount_name": "test-configmap-ssl",
                        "mount_path": "/var/data/"
                    }
                }
            },
            "ports": {
                "http": {
                    "port": "80",
                    "protocol": "tcp"
                }
            },
            "size": {
                "cpu": "100m",
                "memory": "100m"
            },
            "health_checks": {
                "start_up_time": 10,
                "timeout": 100,
                "period": 10,
                "port": "80"
            },
            "autoscaling": {
                "ram_threshold": 90,
                "max": 10,
                "min": 10
            }
        }
    },
    "advanced": {
        "gcp": {
            "iam": {
                "bucket_list": {
                    "role": "roles/xxxxxxxxxxxx"
                }
            }
        },
        "common": {
           "include_common_env_variables": true,
           "app_chart": {
            "values": {
                "hpa": {
                    "behaviour": {}
                },
                "node": {
                    "affinity": {
                        "preferred": {
                            "key": {
                                "key": "",
                                "operator": "In",
                                "topology_key": "",
                                "values": [],
                                "weight": ""
                            }
                        }
                    }
                },
                "enable_service_links": false,
                "keda": {
                    "triggers": {
                        "random-key": {
                            "type": "",
                            "metadata": {}
                        }
                    }
                }
            }
           }
        }

    }
}