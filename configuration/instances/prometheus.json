{
  "kind": "configuration",
  "for": "prometheus",
  "version": "latest",
  "lifecycle": "ENVIRONMENT_BOOTSTRAP<default specified in module, need not be overriden>",
  "disabled": false,
  "provided": true,
  "spec": {
    "diskSize": "100G",
    "cpu": 2,
    "memory": "3GB"
  },
  "out": {
    "comment": "Will be generated, or can be provided",
    "interface": {
      "main": {
        "port": 1000,
        "host": "prometheus.namespace",
        "username": "user",
        "password": "password"
      }
    }
  }
}